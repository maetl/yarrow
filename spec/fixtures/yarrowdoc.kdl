content {
  directory "~/Documents/Projects"
  //module null
  //module false
  //module ""
  module "Project::Model"
  expansions {
    file_map container="pages" extensions=".md"
    file_map container="notebooks" collection="notebook" entry="note"
    file_map collection="blog" entry="post"
  }
}

output {
  template {
    engine "liquid"
    directory "templates"
  }
  target {
    web "www"
  }
}